<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:kelaodi="http://schemas.android.com/apk/res-auto"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">

    <TextView
        android:background="#355E9E"
        android:layout_width="match_parent"
        android:layout_height="45dip"
        android:gravity="center"
        android:text="设置中心"
        android:textSize="24sp"
        android:textColor="#EBC950"/>

    <kelaodi.shenmesafe.ui.SettingView
        android:id="@+id/sv_setting_update"
        kelaodi:timu="自动更新设置"
        kelaodi:checked_text="自动更新设置已开启"
        kelaodi:unchecked_text="自动更新设置未开启"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <kelaodi.shenmesafe.ui.SettingView
        android:id="@+id/sv_setting_Opentest"
        kelaodi:timu="开启密码验证"
        kelaodi:checked_text="密码验证设置已开启"
        kelaodi:unchecked_text="密码验证设置未开启"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</LinearLayout>